{
  "name": "myantd",
  "author": "zhoubichuan",
  "version": "1.0.0",
  "description": "antd二次封装，适应不同的风格",
  "main": "lib/index.js",
  "module": "esm/index.d.ts",
  "types": "lib/index.d.ts",
  "license": "MIT",
  "scripts": {
    "dev": "dumi dev",
    "start": "yarn && dumi dev",
    "lint:md": "lint-md *.md",
    "lint:fix": "lint-md *.md --fix",
    "deploy": "bash ./scripts/deploy.sh",
    "build": "dumi build",
    "docs:deploy": "gh-pages -d docs-dist",
    "clean:esm": "rimraf esm",
    "clean:types": "rimraf types",
    "build:esm": "gulp build",
    "build:cjs": "gulp build --cjs",
    "build:all": "npm-run-all clean:* --parallel build:types --parallel build:*",
    "build:types": "tsc -d --emitDeclarationOnly  --declarationDir ./types",
    "deploy:all": "npm run docs:build && npm run docs:deploy",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage",
    "commit": "git-cz",
    "release": "yarn && standard-version && npm run build && npm publish",
    "release:patch": "yarn && standard-version --release-as patch  && npm run build && npm publish",
    "release:minor": "yarn && standard-version --release-as minor  && npm run build && npm publish",
    "release:alpha": "yarn && standard-version --prerelease alpha && npm run build && npm publish --tag alpha",
    "release:patch-alpha": "yarn && standard-version --prerelease alpha --release-as patch && npm run build && npm publish --tag alpha"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zhoubichuan/web-react.git"
  },
  "homepage": "https://github.com/zhoubichuan/web-react#readme",
  "keywords": [],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "dependencies": {
    "@amap/amap-jsapi-loader": "0.0.7",
    "@ant-design/icons": "^4.7.0",
    "antd": "^4.17.2",
    "echarts": "^5.3.3",
    "ol": "^7.0.0"
  },
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.9.6",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/plugin-proposal-decorators": "7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "7.9.6",
    "@babel/plugin-transform-runtime": "7.9.6",
    "@babel/preset-env": "7.9.6",
    "@babel/preset-react": "7.9.4",
    "@babel/preset-typescript": "7.9.0",
    "@babel/runtime-corejs2": "7.10.2",
    "@testing-library/jest-dom": "5.11.9",
    "@testing-library/react": "11.2.5",
    "@testing-library/react-hooks": "5.1.0",
    "@types/node": "13.7.7",
    "@umijs/plugin-sass": "^1.1.1",
    "@umijs/test": "3.4.22",
    "babel-jest": "25.1.0",
    "babel-loader": "8.0.6",
    "babel-plugin-import": "1.13.0",
    "babel-preset-react-app": "9.1.2",
    "classnames": "2.2.6",
    "commitizen": "4.2.3",
    "cross-env": "7.0.0",
    "cz-conventional-changelog": "3.3.0",
    "dumi": "^1.1.34",
    "dumi-theme-default": "^1.1.17",
    "eslint": "6.8.0",
    "gh-pages": "3.2.0",
    "glob": "7.1.6",
    "gulp": "4.0.2",
    "gulp-babel": "8.0.0",
    "gulp-less": "4.0.1",
    "http-proxy-middleware": "1.0.1",
    "husky": "4.3.0",
    "identity-obj-proxy": "3.0.0",
    "jest": "26.6.3",
    "jest-canvas-mock": "2.3.1",
    "less": "3.11.1",
    "less-loader": "5.0.0",
    "lint-md": "^0.1.1",
    "lint-staged": "10.5.4",
    "mockjs": "1.1.0",
    "moment": "2.24.0",
    "np": "6.2.0",
    "npm-run-all": "4.1.5",
    "prettier": "1.19.1",
    "prism-react-renderer": "^1.1.1",
    "prismjs": "^1.21.0",
    "rimraf": "3.0.2",
    "standard-version": "9.1.1",
    "styled-components": "5.2.0",
    "typescript": "4.2.4",
    "yorkie": "2.0.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-commit": "git update-index --again"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "semi": false
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/jest-setup.ts"
    ],
    "moduleNameMapper": {
      "^@components(.*)$": "<rootDir>/src$1",
      "\\.(css|less)$": "identity-obj-proxy"
    }
  },
  "resolutions": {
    "@types/react": "^16.14.2"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  }
}
